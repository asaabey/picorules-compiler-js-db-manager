{
  "name": "picorules-compiler-js-db-validator",
  "version": "0.1.0",
  "description": "Database validation tool for picorules-compiler-js-core - executes compiled SQL against Oracle and SQL Server",
  "private": true,
  "type": "module",
  "scripts": {
    "validate:oracle": "tsx src/validate-oracle.ts",
    "validate:mssql": "tsx src/validate-mssql.ts",
    "validate:all": "npm run validate:oracle && npm run validate:mssql",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "picorules",
    "sql",
    "validation",
    "oracle",
    "mssql",
    "database"
  ],
  "author": "TKC",
  "license": "MIT",
  "dependencies": {
    "picorules-compiler-js-core": "file:../picorules-compiler-js-core",
    "oracledb": "^6.0.0",
    "mssql": "^10.0.0",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "tsx": "^4.0.0",
    "vitest": "^1.0.0",
    "@types/node": "^20.0.0"
  }
}
