{
  "name": "picorules-compiler-js-db-manager",
  "version": "0.1.3",
  "description": "Database management tool for picorules-compiler-js-core - executes compiled SQL against Oracle, SQL Server and PostgreSQL",
  "type": "module",
  "scripts": {
    "validate:oracle": "tsx src/validate-oracle.ts",
    "validate:mssql": "tsx src/validate-mssql.ts",
    "validate:all": "npm run validate:oracle && npm run validate:mssql",
    "test:prb": "tsx src/test-prb-file.ts",
    "test:all": "tsx src/test-all-ruleblocks.ts",
    "test:all:compile-only": "tsx src/test-all-ruleblocks.ts --compile-only",
    "test": "vitest run",
    "test:watch": "vitest",
    "publish:npm": "bash scripts/publish.sh"
  },
  "keywords": [
    "picorules",
    "sql",
    "database",
    "oracle",
    "mssql",
    "postgresql",
    "manager"
  ],
  "author": "TKC",
  "license": "MIT",
  "dependencies": {
    "@types/pg": "^8.16.0",
    "dotenv": "^16.0.0",
    "mssql": "^10.0.0",
    "oracledb": "^6.0.0",
    "pg": "^8.16.3",
    "picorules-compiler-js-core": "^1.0.6"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
